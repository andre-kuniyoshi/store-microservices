// OcelotApiGateway
{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning",
        "Microsoft.EntityFrameworkCore": "Debug"
      }
    }
  },

  "ElasticConfiguration": {
    "Uri": "http://elasticsearch"
  },

  "OpeniddictConfigs": {
    "RSAPublicKeyPath": "public-key.pem",
    "Issuer": "http://identity.localhost/"
  }
}
